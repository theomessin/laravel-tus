{
    "name": "theomessin/laravel-tus",
    "description": "",
    "keywords": [
        "theomessin",
        "laravel-tus"
    ],
    "homepage": "https://github.com/theomessin/laravel-tus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Theodore Messinezis",
            "email": "theodore@messinezis.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0",
        "slevomat/coding-standard": "~5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Theomessin\\Tus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theomessin\\Tus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theomessin\\Tus\\TusServiceProvider"
            ]
        }
    }
}
